{
  "name": "openhab-persistence-export",
  "version": "0.0.0",
  "description": "A web-based wizard for exporting openHAB persistence data.",
  "scripts": {
    "dev": "parcel serve index.html --open",
    "prebuild": "npm run lint && rm -rf dist",
    "build": "parcel build index.html --dist-dir dist --public-url ./ --no-source-maps --detailed-report",
    "prepreview": "npm run build",
    "preview": "npx http-server dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "Florian Hotze",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "flatpickr": "^4.6.13"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "parcel": "^2.9.3",
    "prettier": "^3.6.2"
  }
}
